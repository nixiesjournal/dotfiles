gtk-xft-rgba="rgb"
gtk-color-scheme="fg_color:#B3B3A1\nbg_color:#333333\nselected_fg_color:#A3CACC\nbase_color:#323232\nselected_bg_color:#1E2320\ntext_color:#B3B3A1\n"
